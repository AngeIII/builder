{
  "name": "vcwb",
  "version": "2.3.0-dev",
  "description": "VC Website Builder",
  "main": "index.php",
  "repository": {
    "type": "git",
    "url": "git@gitlab.com:visual-composer-website-builder/builder.git"
  },
  "scripts": {
    "test": "BABEL_ENV=test BABEL_JEST_STAGE=0 bash ./tests/jest/devElements/_cloneScript.sh && npx jest --env=jsdom",
    "standard": "standard",
    "build": "webpack --config webpack.config.4x.babel.js --progress --colors",
    "build-production": "webpack --config webpack.config.production.4x.babel.js -p --silent",
    "build-production-silent": "webpack --config webpack.config.production.4x.babel.js -p --silent",
    "watch": "webpack --config webpack.config.4x.babel.js --progress --colors --watch",
    "watch-assets-and-elements": "vc watch-elements-and-assets",
    "build-settings": "webpack --config webpack-settings.config.4x.babel.js --progress --colors",
    "build-settings-production": "webpack --config webpack-settings.config.production.4x.babel.js -p --silent",
    "watch-settings": "webpack --config webpack-settings.config.4x.babel.js --progress --colors --watch"
  },
  "author": "@wpb",
  "license": "GPL-2.0+",
  "engines": {
    "node": ">=6",
    "npm": ">=3.8"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-eslint": "8.2.6",
    "babel-jest": "23.4.0",
    "babel-loader": "7.1.5",
    "babel-plugin-lodash": "3.3.4",
    "babel-plugin-rewire": "1.1.0",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-1": "6.24.1",
    "babel-register": "6.26.0",
    "babel-runtime": "6.26.0",
    "classnames": "2.2.6",
    "color-blend": "git://github.com/wpbakery/color-blend.git#5dd059a6dbdc6754f53f4eca09800953534d8322",
    "css-loader": "1.0.0",
    "eslint": "5.1.0",
    "eslint-config-standard": "11.0.0",
    "eslint-config-standard-jsx": "5.0.0",
    "eslint-config-standard-react": "6.0.0",
    "eslint-plugin-react": "7.10.0",
    "exports-loader": "0.7.0",
    "expose-loader": "0.7.5",
    "extract-text-webpack-plugin": "4.0.0-beta.0",
    "file-loader": "1.1.11",
    "fs-extra": "7.0.0",
    "jest-cli": "23.4.1",
    "jquery.caret": "git://github.com/ichord/caret.js.git#341fb20b6126220192b2cd226836cd5d614b3e09",
    "less-loader": "4.1.0",
    "npm": "^6.2.0",
    "postcss-functions": "git://github.com/wpbakery/postcss-functions.git#8aa5a44c1c07c522f704ceaa6794b74682e444f1",
    "postcss-prefix-url": "git://github.com/wpbakery/postcss-prefix-url.git#60f177340ab35fce12cf4172752bd484ece3c7df",
    "qunitjs": "2.4.1",
    "standard": "11.0.1",
    "string-replace-webpack-plugin": "0.1.3",
    "style-loader": "0.21.0",
    "tinycolor2": "1.4.1",
    "uglifyjs-webpack-plugin": "1.2.7",
    "url-loader": "1.0.1",
    "virtual-module-webpack-plugin": "0.4.0",
    "webpack": "4.16.1",
    "webpack-cli": "3.0.8"
  },
  "dependencies": {
    "autoprefixer": "9.0.0",
    "base-64": "0.1.0",
    "codemirror": "5.39.0",
    "combokeys": "3.0.0",
    "csslint": "1.0.5",
    "eslint-plugin-promise": "3.8.0",
    "eslint-plugin-standard": "3.1.0",
    "event-emitter": "0.3.5",
    "form-serialize": "0.7.2",
    "htmlhint": "0.9.13",
    "identity-obj-proxy": "3.0.0",
    "immutable": "3.8.2",
    "imports-loader": "0.8.0",
    "jquery": "3.3.1",
    "jshint": "2.9.5",
    "less": "3.7.1",
    "lodash": "4.17.10",
    "medium-editor": "5.23.3",
    "mobile-detect": "1.4.2",
    "object-hash": "1.3.0",
    "pako": "1.0.6",
    "perfect-scrollbar": "1.4.0",
    "postcss": "7.0.0",
    "postcss-advanced-variables": "2.3.3",
    "postcss-clean": "1.1.0",
    "postcss-color-function": "4.0.1",
    "postcss-conditionals": "2.1.0",
    "postcss-custom-media": "6.0.0",
    "postcss-custom-properties": "7.0.0",
    "postcss-each": "0.10.0",
    "postcss-loader": "2.1.6",
    "postcss-math": "0.0.10",
    "postcss-nested": "3.0.0",
    "prop-types": "15.6.2",
    "raw-loader": "0.5.1",
    "react": "16.4.1",
    "react-addons-shallow-compare": "15.6.2",
    "react-color": "2.14.1",
    "react-custom-scrollbars": "4.2.1",
    "react-dom": "16.4.1",
    "react-sortable-hoc": "0.8.3",
    "react-test-renderer": "16.4.1",
    "react-textarea-autosize": "7.0.3",
    "react-tinymce": "0.7.0",
    "reactcss": "1.2.3",
    "simple-react-modal": "0.5.1",
    "slick-carousel": "1.8.1",
    "striptags": "3.1.1",
    "vc-cake": "0.7.0",
    "webfontloader": "1.6.28"
  },
  "babel": {
    "presets": [
      "react",
      "es2015",
      "stage-1"
    ],
    "plugins": [
      "transform-runtime"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": [
      "standard",
      "standard-react"
    ],
    "rules": {
      "jsx-quotes": [
        2,
        "prefer-single"
      ],
      "import/no-webpack-loader-syntax": "off"
    }
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "public/config/",
      "public/sources/assetsLibrary",
      "tests/",
      "tools/"
    ],
    "rules": {
      "jsx-quotes": [
        2,
        "prefer-single"
      ],
      "import/no-webpack-loader-syntax": "off"
    }
  },
  "jest": {
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/devElements/",
      "__mocks__"
    ],
    "testRegex": "(/tests/jest/.*)\\.jsx?$",
    "moduleNameMapper": {
      "^raw-loader\\!": "<rootDir>/tests/jest/__mocks__/fileMock.js",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/tests/jest/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy",
      "^public(.*)$": "<rootDir>/public$1"
    }
  }
}
