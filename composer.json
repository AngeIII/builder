{
  "autoload": {
    "psr-4": {
      "VisualComposer\\": "visualcomposer/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ComposerHooks\\": "tools/php-composer/"
    }
  },
  "scripts": {
    "post-update-cmd": [
      "ComposerHooks\\Registry::postUpdateCmd",
      "php tools/vcwb-languages/cli.php"
    ],
    "post-install-cmd": [
      "ComposerHooks\\Registry::postInstallCmd",
      "php tools/vcwb-languages/cli.php"
    ],
    "post-autoload-dump": [
      "ComposerHooks\\Registry::postUpdateCmd"
    ]
  },
  "require": {
    "ext-exif": "*"
  }
}
