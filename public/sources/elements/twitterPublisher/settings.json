      "values": [
        {
          "label": "Embedded Grid",
          "value": "grid"
        },
        {
          "label": "Embedded Tweet",
          "value": "tweet"
        },
        {
          "label": "Embedded Timeline",
          "value": "timeline"
        },
        {
          "label": "Twitter Button",
          "value": "button"
        }
      ]
