    "options": {
      "label": "Add twitter grid url",
      "onChange": {
        "twitterOptions": [
          {
            "rule": "value",
            "options": "grid",
            "actions": [
              {
                "action": "toggleVisibility"
              }
            ]
          }
        ]
      }
    }
