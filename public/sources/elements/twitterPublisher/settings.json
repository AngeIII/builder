  "editFormTab1": {
    "type": "group",
    "access": "protected",
    "value": [
      "twitterOptions",
      "gridUrl",
      "tweetUrl",
      "timelineUrl",
      "customClass",
      "customOptions",
      "alignment",
      "tweetCount",
      "width"
