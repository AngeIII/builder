  "metaIntro": {
    "type": "textarea",
    "access": "protected",
    "value": "Short intro"
  },
  "metaDescription": {
    "type": "textarea",
    "access": "protected",
    "value": "Long description"
  },
  "metaPreviewDescription": {
    "type": "textarea",
    "access": "protected",
    "value": "Medium preview description"
  },
  "metaPreview": {
    "type": "attachimage",
    "access": "protected",
    "value": "preview.jpg"
  },
  "metaThumbnail": {
    "type": "attachimage",
    "access": "protected",
    "value": "thumbnail.jpg"
  },
  "designOptions": {
    "type": "designOptions",
    "access": "public",
    "value": {},
    "options": {
      "label": "Design Options"
    }
  },
  "selector": {
    "type": "dropdown",
    "access": "public",
    "value": "id",
    "options": {
      "label": "Choose selector",
      "description": "Show a single product by ID or SKU.",
      "values": [
        {
          "label": "ID",
          "value": "id"
        },
        {
          "label": "SKU",
          "value": "sku"
        }
      ]
    }
  },
  "productId": {
    "type": "string",
    "access": "public",
    "value": "",
    "options": {
      "label": "Product ID",
      "onChange": {
        "rules": {
          "selector": {
            "rule": "value",
            "options": {
              "value": "id"
            }
          }
        },
        "actions": [
          {
            "action": "toggleVisibility"
          }
        ]
      }
    }
  },
  "productSku": {
    "type": "string",
    "access": "public",
    "value": "",
    "options": {
      "label": "Product SKU",
      "onChange": {
        "rules": {
          "selector": {
            "rule": "value",
            "options": {
              "value": "sku"
            }
          }
        },
        "actions": [
          {
            "action": "toggleVisibility"
          }
        ]
      }
    }
  },
  "editFormTab1": {
    "type": "group",
    "access": "protected",
    "value": [
      "selector",
      "productId",
      "productSku"
    ],
    "options": {
      "label": "Product"
    }
  },
  "metaEditFormTabs": {
    "type": "group",
    "access": "protected",
    "value": [
      "editFormTab1",
      "designOptions"
    ]
  },
  "relatedTo": {
    "type": "group",
    "access": "protected",
    "value": [
      "General"
    ]
  }
