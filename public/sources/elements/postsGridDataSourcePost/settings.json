{
  "name": {
    "type": "string",
    "access": "protected",
    "value": "Posts"
  },
  "attsCategory": {
    "type": "dropdown",
    "access": "public",
    "value": "",
    "options": {
      "label": "Select category",
      "global": "vcvPostsGridSourcePostCategories"
    }
  },
  "attsOffset": {
    "type": "string",
    "access": "public",
    "value": "",
    "options": {
      "label": "Offset"
    }
  },
  "attsLimit": {
    "type": "string",
    "access": "public",
    "value": "10",
    "options": {
      "label": "Limit"
    }
  },
  "attsSticky": {
    "type": "toggle",
    "access": "public",
    "value": false,
    "options": {
      "label": "Add sticky posts"
    }
  },
  "postsGridDataSource": {
    "type": "group",
    "access": "protected",
    "value": [
      "attsCategory",
      "attsOffset",
      "attsLimit",
      "attsSticky"
    ],
    "options": {
      "label": "Posts Grid"
    }
  },
  "metaEditFormTabs": {
    "type": "group",
    "access": "protected",
    "value": [
      "postsGridDataSource"
    ]
  }
